<UserControl x:Class="Bazy.UstawieniaPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Bazy"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="600">
    <Grid>
        <Label Content="Zmiana hasła" HorizontalAlignment="Left" Margin="222,52,0,0" VerticalAlignment="Top" Width="82"/>
        <PasswordBox x:Name="txtNoweHaslo" HorizontalAlignment="Left" Margin="203,83,0,0" VerticalAlignment="Top" Width="120" BorderBrush="#3AB19B"/>
        <PasswordBox x:Name="txtPotwierdzNoweHaslo" HorizontalAlignment="Left" Margin="203,120,0,0" VerticalAlignment="Top" Width="120" BorderBrush="#3AB19B"/>
        <Button Content="Zmień hasło" HorizontalAlignment="Left" Margin="194,163,0,0" VerticalAlignment="Top" Style="{StaticResource buttonZaloguj}" Width="138" Click="Button_Click"/>
        <Label Content="Nowe hasło:" HorizontalAlignment="Left" Margin="67,83,0,0" VerticalAlignment="Top"/>
        <Label Content="Podwierdz nowe hasło:" HorizontalAlignment="Left" Margin="67,116,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
